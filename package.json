{
  "name": "st-mdc",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "module": "dist/esm/index.js",
  "main": "dist/index.js",
  "unpkg": "dist/ionic.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "scss": "npx node-sass src/theme.scss -o www --include-path node_modules && cp src/custom.css www",
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll"
  },
  "dependencies": {
    "@material/button": "^1.1.1",
    "@material/card": "^2.1.1",
    "@material/checkbox": "^2.1.1",
    "@material/form-field": "^2.1.1",
    "@material/icon-button": "^1.1.0",
    "@material/textfield": "^1.1.1"
  },
  "devDependencies": {
    "@stencil/core": "^0.18.1",
    "@stencil/sass": "^0.2.1",
    "node-sass": "^4.12.0"
  },
  "license": "MIT"
}
